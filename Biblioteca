a) SELECT FROM BIBLI.LIVROS WHERE AUTOR="Machado de Assis";

b) SELECT FROM BIBLI.LIVROS WHERE LIVRO="BANCO DE DADOS";

c) SELECT ALUNO, EMPRESTIMO_DATA,FUNCIONARIO FROM BIBLI.EMPRESTIMOS, BIBLI.ALUNOS, BIBLI.FUNCIONARIOS WHERE EMPRESTIMOS.idALUNOS ALUNOS.idALUNOS AND EMPRESTIMOS.idFUNCIONARIOS FUNCIONARIOS.idFUNCIONARIOS ORDER BY EMPRESTIMO_DATA;

d) SELECT EMPRESTIMOS.idEMPRESTIMOS, LIVRO,EMPRESTIMO_DATA,DEVOLUCAO_DATA FR BIBLI.EMPRESTIMO_ITENS, BIBLI.EMPRESTIMOS, BIBLI.EXEMPLARES, BIBLI.LIVROS WHERE BIBLI.EMPRESTIMOS.IDEMPRESTIMOS-BIBLI.ITENSEMPRESTIMO.IDEMPRESTI AND BIBLI.EMPRESTIMO_ITENS.id EXEMPLARES BIBLI.EXEMPLARES.idEXEMPLARES AND BIBLI.LIVROS.idLIVROS BIBLI.EXEMPLARES.LIVROS_idLIVROS AND EMPRESTIMO_DATA < "2021-01-07" AND DEVOLUCAO_DATA IS NULL ORDER BY LIVRO;

e) SELECT EMPRESTIMOS.idEMPRESTIMOS, EXEMPLARES.idEXEMPLARES, ALUNO, LIVRO, EMPRESTIMO_DATA, PREVDEV_DATA, DEVOLUCAO_DATA FROM BIBLI.EMPRESTIMO_ITENS,BIBLI.EMPRESTIMOS, BIBLI.EXEMPLARES, BIBLI.LIVROS, BIBLI.ALUNOS WHERE BIBLI.EMPRESTIMOS.idALUNOS BIBLI.ALUNOS.idALUNOS ANDAND BIBLI.EMPRESTIMOS. IDEMPRESTIMOS BIBLI.EMPRESTIMO_ITENNS.IDEMPRESTIMOS > EMPRESTIMOS AND BIBLI. EMPRESTIMO_ITENS.idEXEMPLARES BIBLI.EXEMPLARES. idEXEMPLARES AND BIBLI.LIVROS.idLIVROS BIBLI EXEMPLARES. LIVROS_idLIVROS >EXEMPLARES AND PREVDEV_DATA <2821-01-11" AND DEVOLUCAO_DATA IS NULL > FUNCIONARIOS ORDER BY LIVRO;

F)SELECT DISTINCT ALUNO, LIVRO FROM BIBLI. EMPRESTIMO_ITENS, BIBLI. EMPRESTIMOS, BIBLI.EXEMPLARES, BIBLI. LIVROS, BIBLI.ALUNOS WHERE BIBLI. EMPRESTIMOS.idALUNOS BIBLI. ALUNOS. IdALUNOS AND BIBLI. EMPRESTIMOS. IDEMPRESTIMOS BIBLI.EMPRESTIMO_ITENS.IDEMPRESTIMOS AND BIBLI.EMPRESTIMO_ITENS.idEXEMPLARES BIBLI.EXEMPLARES. idEXEMPLARES AND BIBLI. LIVROS.idLIVROS BIBLI.EXEMPLARES. LIVROS_idLIVROS ORDER BY ALUNO, LIVRO;

G) SELECT DISTINCT ALUNO, LIVRO FROM BIBLI.EMPRESTIMO_ITENS, BIBLIOTECA. EMPRESTIMOS, BIBLI. EXEMPLARES, BIBLI. LIVROS, BIBLI. ALUNOS WHERE BIBLI. EMPRESTIMOS. idALUNOS BIBLI.ALUNOS.idALUNOS

AND BIBLI. EMPRESTIMOS. IDEMPRESTIMOS BIBLI.EMPRESTIMO_ITENS.IDEMPRESTIMOS AND BIBLI. EMPRESTIMO_ITENS.idEXEMPLARES BIBLI.EXEMPLARES. idEXEMPLARES AND BIBLI. LIVROS.idLIVROS BIBLI.EXEMPLARES. LIVROS_idLIVROS AND LIVRO="P00" ORDER BY ALUNO, LIVRO;

h) SELECT ALUNO, COUNT(idEMPRESTIMOS) AS QTDEEMPRESTIMOS FROM BIBLI.EMPRESTIMOS, BIBLI.ALUNOS WHERE EMPRESTIMOS.idALUNOS ALUNOS.idALUNOS GROUP BY ALUNO;

I) SELECT ALUNO,count(idEXEMPLARES) AS QTDEEXEMPLARES FROM BIBLI.EMPRESTIMOS, BIBLI.ALUNOS, BIBLI.EMPRESTIMO_ITENS WHERE EMPRESTIMOS.idALUNOS ALUNOS.idALUNOS AND EMPRESTIMOS.idEMPRESTIMOS= EMPRESTIMO_ITENS.idEMPRESTIMOS
GROUP BY ALUNO;

J) SELECT LIVRO,COUNT(idEXEMPLARES) AS QTDEEXEMPLARES FROM BIBLI.EXEMPLARES, BIBLI.LIVROS WHERE (BIBLI.EXEMPLARES.LIVROS_idLIVROS-BIBLI.LIVROS.idLIVROS) GROUP BY LIVRO HAVING (QTDEEXEMPLARES=1);

K) SELECT ALUNO,SUM(MULTA) AS TOTMULTAS FROM BIBLI.ALUNOS, BIBLI.EMPRESTIMOS, BIBLI. EMPRESTIMO_ITENS WHERE (ALUNOS.idALUNOS EMPRESTIMOS.idALUNOS) AND (EMPRESTIMOS.idEMPRESTIMOS= EMPRESTIMO_ITENS.idEMPRESTIMOS) GROUP BY ALUNO;

